<?xml version="1.0"?>
<assembly>
	<id>rename-files</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>src/main/java</directory>
			<outputDirectory></outputDirectory>
			<useDefaultExcludes>true</useDefaultExcludes>
			<includes>
				<include>by/training/antlib.xml</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target/classes</directory>
			<outputDirectory></outputDirectory>
			<useDefaultExcludes>true</useDefaultExcludes>
			<includes>
				<include>by/training/*.class</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>